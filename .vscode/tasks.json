{
    // See https://go.microsoft.com/fwlink/?LinkId=733558 
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cargo",
            "label": "cargo release",
            "command": "cargo",
            "args": [
                "build",
                "release"
            ],
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "type": "cargo",
            "label": "cargo bench",
            "command": "cargo",
            "args": [
                "+nightly",
                "bench"
            ],
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "type": "cargo",
            "label": "cargo run",
            "command": "cargo",
            "args": [
                "run"
            ],
            "problemMatcher": [
                "$rustc"
            ]
        }
    ]
}